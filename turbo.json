{
	"$schema": "https://turborepo.org/schema.json",
	"baseBranch": "origin/master",
	"pipeline": {
		"dev": {
			"cache": false
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**"]
		},
		"app#build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**"]
		},
		"website#build": {
			"dependsOn": ["^build", "$NEXT_PUBLIC_APP_URL"],
			"outputs": [".next/**"]
		},
		"server#build": {
			"dependsOn": [
				"^build",
				"$GITHUB_ID",
				"$GITHUB_SECRET",
				"$OAUTH_REDIRECT_URL",
				"$JWT_EXPIRATION_TIME_SECONDS",
				"$JWT_SECRET",
				"$PORT",
				"$DB_HOST",
				"$DB_PORT",
				"$DB_USER",
				"$DB_PASS",
				"$DB_NAME"
			],
			"outputs": [".next/**"]
		},
		"test": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"lint": {
			"outputs": []
		},
		"deploy": {
			"dependsOn": ["^build"],
			"outputs": []
		}
	}
}
